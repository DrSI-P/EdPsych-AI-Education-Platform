# Production environment variables for Vercel deployment
# This is an example file. Create a .env.production file with actual values.

# Database connection
DATABASE_URL="postgresql://username:password@host:port/database?sslmode=require"

# Next Auth configuration
NEXTAUTH_URL="https://edpsychconnect.com"
NEXTAUTH_SECRET="your-nextauth-secret"

# Application mode
MAINTENANCE_MODE="false"
NODE_ENV="production"

# Prisma configuration
SKIP_PRISMA_MIGRATIONS="false"
SKIP_DB_CONNECT="false"
NEXT_PUBLIC_SKIP_DB_INIT="false"

# Build optimization
NEXT_SKIP_STATIC_GENERATION="true"
NEXT_TELEMETRY_DISABLED="1"

# Public variables
NEXT_PUBLIC_SITE_URL="https://edpsychconnect.com"
NEXT_PUBLIC_APP_URL="https://edpsychconnect.com"
NEXT_PUBLIC_APP_NAME="EdPsych Connect"

# Payment processing
STRIPE_TEST_SECRET_KEY="your-stripe-test-secret-key"
STRIPE_WEBHOOK_SECRET="your-stripe-webhook-secret"
NEXT_PUBLIC_HEYGEN_API_KEY="your-heygen-api-key"
NEXT_PUBLIC_HEYGEN_API_URL="https://api.heygen.com"