{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "NEXT_SKIP_STATIC_GENERATION": "true",
    "NEXT_PUBLIC_SITE_URL": "https://edpsychconnect.com",
    "NEXT_PUBLIC_APP_URL": "https://edpsychconnect.com",
    "STRIPE_TEST_SECRET_KEY": "sk_test_51NxSamLqkIAIGWRZGBvmK6YVqzjZLV8Ugs9LmCIcMIEyUAQCwK7HEfnWYksDYJqSGnOK5PEfLFfJWKvLMXpGQjWR00ORLYPDtL"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/blog/:slug",
      "destination": "/blog/[slug]"
    }
  ]
}
