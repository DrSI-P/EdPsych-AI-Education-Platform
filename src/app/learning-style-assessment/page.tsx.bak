      previousAssessments: 2
    },
    { 
      id: 's2', 
      name: 'James Wilson', 
      year: 'Year 7',
      age: 12,
      avatar: 'ðŸ‘¦ðŸ½',
      previousAssessments: 1
    },
    { 
      id: 's3', 
      name: 'Sophia Chen', 
      year: 'Year 3',
      age: 8,
      avatar: 'ðŸ‘§ðŸ»',
      previousAssessments: 0
    },
    { 
      id: 's4', 
      name: 'Mohammed Ali', 
